<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TicketIndexController">
  <children>
    <AnchorPane layoutX="103.0" layoutY="14.0" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Text layoutX="144.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Marta" />
            <Text layoutX="62.0" layoutY="123.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please take a ticket or Insert your ticket" />
            <Button fx:id="takeTicket" layoutX="119.0" layoutY="167.0" mnemonicParsing="false" onAction="#newTicket" text="Take a Ticket Number" />
            <TextField fx:id="keyEntry" layoutX="105.0" layoutY="253.0" promptText="Insert ticket number" />
            <Button fx:id="submitButton" layoutX="177.0" layoutY="320.0" mnemonicParsing="false" onAction="#submitPressed" text="Submit" />
      </children>
    </AnchorPane>
  </children>
</AnchorPane>
